<div class="container">

<%= simple_form_for(@pool) do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :street %>
  <%= f.input :city %>
  <%= f.input :zip_code %>
  <%= f.input :category %>
  <%= f.input :qm %>
  <%= f.input :price_per_day %>
  <%= f.input :max_capacity %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit %>
<% end %>
</div>

